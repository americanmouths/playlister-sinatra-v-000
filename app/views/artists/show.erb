<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
<title></title>
  </head>
<body>
  <h1> <%= @artist.name %> </h1>

  <% @artist.songs.each do |song| %>
    <a href="/songs/<%= song.slug %>">
    <%= song.name %>
  <% end %>

  <% @artist.genres.each do |genre| %>
    <a href="/genres/<%= genre.slug %>">
    <%= genre.name %>
  <% end %>
    
</body>
</html>
